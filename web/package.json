    "chakra-ui-autocomplete": "^1.4.5",
    "dayjs": "^1.11.5",
    "formik": "^2.2.9",
    "framer-motion": "^7.5.3",
    "msw": "^0.47.4",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-contexify": "^5.0.0",
    "react-dom": "^18.2.0",
    "react-easy-crop": "^4.5.1",
